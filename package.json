{
  "name": "algorithm-tailwind-express-ejs",
  "version": "1.0.0",
  "description": "```\r npm init -y\r npm i express\r npm i ejs\r npm i nodemon --save-dev\r npx tailwindcss init\r npm i -D tailwindcss@latest postcss@latest autoprefixer@latest\r ```",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js --watch views -e ejs algorithm-tailwind-express-ejs/index.js --exec \"npm run build\"",
    "initCss": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/styles.css --watch",
    "build": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/styles.css && nodemon index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MystioDev/algorithm-tailwind-express-ejs.git"
  },
  "bugs": {
